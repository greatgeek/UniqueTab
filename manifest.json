{
    "manifest_version": 3,
    "name": "Unique Tab Extension",
    "version": "1.0",
    "permissions": [
        "tabs",
        "storage"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon-disabled.png"
          }
        },
        "icons": {
          "16": "images/icon-disabled.png"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
        ]
}